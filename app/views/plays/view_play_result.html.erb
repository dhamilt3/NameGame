<h1> Play Results <%=@play_id%></h1>

<div>
  Draws: <%=@draw_set.count%>
</div>

<div>
  The Play: <%=@user_play%>
</div>

 <div>Correct: <%=@correct_count.to_i%></div>
 <div>Incorrect: <%=@incorrect_count.to_i%></div>
 <div>Accuracy: <%=(@percent)%>%</div>

 <div>
  <table border="1">
    <%@draw_set.each do |draw|%>

    <tr>
      <th>Photo</th>
      <th>Correct_Answer</th>
      <th>Your Answer</th>
      <th>Points</th>
    </tr>

    <tr>
      <%roster_member = Roster.all.where({:id => draw.roster_id}).at(0)%>
      <%img_url = roster_member.image%>

      <td><img src="<%=img_url%>"></td>      
      <td><%=draw.name_match%>
      <td><%=draw.name_attempt%>
      <td><%=draw.draw_result%>
     
    </tr>
    <%end%>
  </table>


 </div>


